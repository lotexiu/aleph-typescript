{
  "name": "aleph-typescript",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "new": "turbo gen workspace",
    "mismatch": "pnpm dlx syncpack list-mismatches",
    "fix-mismatch": "pnpm dlx syncpack fix-mismatches",
    "update-pkgs": "up jest --latest --recursive",
    "git-init": "git submodule update --init --recursive",
    "git-sync": "git pull --recurse-submodules",
		"clean": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' + && pnpm store prune"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.15.3",
    "@vitejs/plugin-react": "^5.1.0",
    "chalk": "^5.6.2",
    "fs-extra": "^11.3.2",
    "ora": "^9.0.0",
    "prettier": "^3.6.2",
    "turbo": "^2.6.0",
    "typescript": "^5.9.2",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "packageManager": "pnpm@10.21.0",
  "engines": {
    "node": ">=18"
  }
}
